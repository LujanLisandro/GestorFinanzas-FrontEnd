# .env.local - Configuración para desarrollo local
# Este archivo NO se sube a Git (ya está en .gitignore)
# Copia este archivo y renómbralo a .env.local para desarrollo

VITE_API_URL=http://localhost:8080/api/v1

# Configuraciones de la aplicación
VITE_APP_NAME="Gestor de Finanzas (Local)"
VITE_APP_VERSION="1.0.0"

# Configuración de Dólar API
VITE_DOLAR_API_URL=https://dolarapi.com/v1

# Configuración de sesión
VITE_SESSION_TIMEOUT=30

# Modo de desarrollo (true para habilitar logs adicionales)
VITE_DEBUG_MODE=true
